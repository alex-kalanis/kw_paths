{
    "name": "alex-kalanis/kw_paths",
    "description": "Paths for accessing targets in KWCMS",
    "keywords": [
        "filesystem",
        "files",
        "path",
        "paths"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "ext-mbstring": "*",
        "php": ">=7.4.0"
    },
    "require-dev": {
        "shipmonk/composer-dependency-analyser": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": ">=9 <10"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "kalanis\\kw_paths\\": "php-src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "php-tests/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
